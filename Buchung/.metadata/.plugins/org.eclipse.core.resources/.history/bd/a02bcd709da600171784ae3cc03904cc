package jdbc;
import java.sql.*;
public class Driver {

	public Driver(){
		try {
			// Connection
			
			Connection con= DriverManager.getConnection("jdbc:mysql://localhost:3306/flightdata","root","Toni0501");
			
			PreparedStatement ps = con.prepareStatement("Select * from airlines;");
			ResultSet rs=ps.executeQuery();
			while(rs.next()){
				System.out.println(rs.getString("id")+ " , "+rs.getString("name")+ " , "+ rs.getString("country"));
			}
		}catch(Exception exc){
			exc.printStackTrace();
		}

	}

}
